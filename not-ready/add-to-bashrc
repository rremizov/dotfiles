export PATH=$PATH:/usr/local/sbin:/usr/sbin:/sbin:~/bin
export EDITOR=vim
alias ranger='TERM=xterm-256color ranger'
alias mutt='TERM=xterm-256color mutt'

# solarize dircolors
eval `dircolors $HOME/.dircolors.ansi`

# Python modules 
export PYTHONPATH=~/python/modules

GPG_TTY=$(tty)
export GPG_TTY

